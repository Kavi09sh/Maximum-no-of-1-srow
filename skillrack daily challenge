def res(n):
    l=[]
    for i in range(n):
        p=list(map(int,input().split()))
        c=0
        for j in p:
           if j==1:
               c+=1 
        l.append(c)
    m=max(l)
    for i in range(len(l)):
       if l[i]==m:
           v=i+1 
           break
    print(v)    
x=int(input())
res(x)
